		switch 	(FEAT_INDUSTRIES, SELF, total_supply_industries, LOAD_PERM(02))	{return;}


//calculate_primary_cargo_stockpile
	switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_stockpile_10, base_extraction_production * LOAD_PERM(220)) {return;}
