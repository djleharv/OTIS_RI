				extraction_primary_cargo_stockpile_17() +
				extraction_primary_cargo_stockpile_18() +
				extraction_primary_cargo_stockpile_19() )
			) {return;}


//calculate cargo consumption
	//primary cargos
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_10, extraction_primary_cargo_stockpile_10()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_11, extraction_primary_cargo_stockpile_11()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_12, extraction_primary_cargo_stockpile_12()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_13, extraction_primary_cargo_stockpile_13()) {return;}
