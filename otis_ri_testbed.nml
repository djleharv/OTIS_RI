		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_24, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_25, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_26, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_27, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_28, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_29, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
	//support cargos
		switch 	(FEAT_INDUSTRIES, SELF, extraction_support_cargo_consumption_30, (extraction_total_production() * support_consumption / 100) / number_of_support_cargos()) {return;}
