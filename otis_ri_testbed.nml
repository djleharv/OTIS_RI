		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_15, extraction_primary_cargo_stockpile_15()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_16, extraction_primary_cargo_stockpile_16()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_17, extraction_primary_cargo_stockpile_17()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_18, extraction_primary_cargo_stockpile_18()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_primary_cargo_consumption_19, extraction_primary_cargo_stockpile_19()) {return;}

	//secondary cargos
		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_20, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
		switch 	(FEAT_INDUSTRIES, SELF, extraction_secondary_cargo_consumption_21, (extraction_total_production() * secondary_consumption / 100) / number_of_secondary_cargos()) {return;}
